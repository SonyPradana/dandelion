{
  "manifest_version": 3,
  "name": "Dandelion - Auto fill form",
  "version": "0.0.1",
  "description": "Speeds up the process of filling out your forms.",
  "host_permissions": [
     "https://*/skrining*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/skrining*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
