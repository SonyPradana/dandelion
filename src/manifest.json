{
  "manifest_version": 3,
  "name": "Dandelion - Auto fill form",
  "version": "1.0",
  "description": "Speeds up the process of filling out your forms.",
  "action": {
    "default_popup": "view/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
      ],
      "js": [
        "browser-polyfill.js",
        "main.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
