    "@rollup/plugin-replace": "^6.0.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "neostandard": "^0.12.2",
    "rimraf": "^6.1.2",
    "rollup": "^4.54.0",
    "web-ext": "^9.2.0"
  },
  "dependencies": {
    "webextension-polyfill": "^0.12.0"
